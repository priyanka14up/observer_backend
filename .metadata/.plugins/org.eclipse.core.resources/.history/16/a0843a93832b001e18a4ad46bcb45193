package Observer20.Dto;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;

import Observer20.repository.AnswerRepo;

public class SubformResponse {
	@Autowired
    private AnswerRepo answerRepo;
	
    private Long subformId;
    private List<Long> answerIds;
    
    
    
    
	public Long getSubformId() {
		return subformId;
	}
	public void setSubformId(Long subformId) {
		this.subformId = subformId;
	}
	public List<Long> getAnswerIds() {
		return answerIds;
	}
	public void setAnswerIds(List<Long> answerIds) {
		this.answerIds = answerIds;
	}
	public void fetchAnswerIdsFromDatabase() {
        
        answerIds= answerRepo.findAnswerIdsBySid(subformId);
    }
	
	@Override
	public String toString() {
		return "SubformResponse [subformId=" + subformId + ", answerIds=" + answerIds + "]";
	}

    // Constructors, getters, setters, etc.

    // Additional methods or annotations may be required to interact with the database.
}